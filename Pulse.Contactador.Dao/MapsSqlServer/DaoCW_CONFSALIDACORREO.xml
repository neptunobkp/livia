<statements>
	<select id="FindAllDaoCW_CONFSALIDACORREO" resultMap="ResultDaoCW_CONFSALIDACORREO">
		 SELECT * FROM CW_CONFSALIDACORREO
	</select>

	<select id="FindDaoCW_CONFSALIDACORREO" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE IDCONFSALIDACORREO = #value#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOBySERVIDORSMTP" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE SERVIDORSMTP = #SERVIDORSMTP#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByPUERTOSERVIDOR" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE PUERTOSERVIDOR = #PUERTOSERVIDOR#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOBySSLHABILITADO" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE SSLHABILITADO = #SSLHABILITADO#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByUSUARIO" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE USUARIO = #USUARIO#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByCONTRASENIA" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE CONTRASENIA = #CONTRASENIA#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByCORREOCASILLASALIDA" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE CORREOCASILLASALIDA = #CORREOCASILLASALIDA#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByNOMBRECASILLASALDA" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE NOMBRECASILLASALDA = #NOMBRECASILLASALDA#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByIDPROPIETARIO" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE IDPROPIETARIO = #IDPROPIETARIO#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByTIPOPROPIETARIO" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE TIPOPROPIETARIO = #TIPOPROPIETARIO#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByUSUARIOCREADOR" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE USUARIOCREADOR = #USUARIOCREADOR#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByFECHACREACION" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE FECHACREACION = #FECHACREACION#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByUSUARIOMODIFICADOR" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE USUARIOMODIFICADOR = #USUARIOMODIFICADOR#
	</select>
	<select id="FindDaoCW_CONFSALIDACORREOByFECHAMODIFICACION" resultMap="ResultDaoCW_CONFSALIDACORREO" extends="FindAllDaoCW_CONFSALIDACORREO">
		WHERE FECHAMODIFICACION = #FECHAMODIFICACION#
	</select>

	<select id="FindByOne" parameterClass="DaoCW_CONFSALIDACORREO" resultMap="ResultDaoCW_CONFSALIDACORREO">
		Select * from CW_CONFSALIDACORREO
		<dynamic prepend="WHERE 1 = 1">
							<isNotNull property="IDCONFSALIDACORREO">
				<isGreaterThan compareValue="0" property="IDCONFSALIDACORREO">
					AND IDCONFSALIDACORREO = #IDCONFSALIDACORREO#
				</isGreaterThan>

			</isNotNull>

							<isNotNull property="SERVIDORSMTP">
					AND SERVIDORSMTP = #SERVIDORSMTP#
			</isNotNull>

							<isNotNull property="PUERTOSERVIDOR">
					AND PUERTOSERVIDOR = #PUERTOSERVIDOR#
			</isNotNull>

							<isNotNull property="SSLHABILITADO">
					AND SSLHABILITADO = #SSLHABILITADO#
			</isNotNull>

							<isNotNull property="USUARIO">
					AND USUARIO = #USUARIO#
			</isNotNull>

							<isNotNull property="CONTRASENIA">
					AND CONTRASENIA = #CONTRASENIA#
			</isNotNull>

							<isNotNull property="CORREOCASILLASALIDA">
					AND CORREOCASILLASALIDA = #CORREOCASILLASALIDA#
			</isNotNull>

							<isNotNull property="NOMBRECASILLASALDA">
					AND NOMBRECASILLASALDA = #NOMBRECASILLASALDA#
			</isNotNull>

								<isGreaterThan compareValue="0" property="IDPROPIETARIO">
					AND IDPROPIETARIO = #IDPROPIETARIO#
				</isGreaterThan>

								<isGreaterThan compareValue="0" property="TIPOPROPIETARIO">
					AND TIPOPROPIETARIO = #TIPOPROPIETARIO#
				</isGreaterThan>

								<isGreaterThan compareValue="0" property="USUARIOCREADOR">
					AND USUARIOCREADOR = #USUARIOCREADOR#
				</isGreaterThan>

				
								<isGreaterThan compareValue="0" property="USUARIOMODIFICADOR">
					AND USUARIOMODIFICADOR = #USUARIOMODIFICADOR#
				</isGreaterThan>

				
			</dynamic>
	</select>

<update id="UpdateDaoCW_CONFSALIDACORREO" >
	UPDATE CW_CONFSALIDACORREO SET 
		SERVIDORSMTP = #SERVIDORSMTP#,
		PUERTOSERVIDOR = #PUERTOSERVIDOR#,
		SSLHABILITADO = #SSLHABILITADO#,
		USUARIO = #USUARIO#,
		CONTRASENIA = #CONTRASENIA#,
		CORREOCASILLASALIDA = #CORREOCASILLASALIDA#,
		NOMBRECASILLASALDA = #NOMBRECASILLASALDA#,
		IDPROPIETARIO = #IDPROPIETARIO#,
		TIPOPROPIETARIO = #TIPOPROPIETARIO#,
		USUARIOCREADOR = #USUARIOCREADOR#,
		FECHACREACION = #FECHACREACION#,
		USUARIOMODIFICADOR = #USUARIOMODIFICADOR#,
		FECHAMODIFICACION = #FECHAMODIFICACION#
	WHERE IDCONFSALIDACORREO = #IDCONFSALIDACORREO#
</update>

<delete id="DeleteDaoCW_CONFSALIDACORREO" >
	DELETE FROM CW_CONFSALIDACORREO
	WHERE IDCONFSALIDACORREO = #IDCONFSALIDACORREO#
</delete>

<select id="GetMaxIdDaoCW_CONFSALIDACORREO" >
SELECT MAX(IDCONFSALIDACORREO) + 1 FROM CW_CONFSALIDACORREO 
</select>

