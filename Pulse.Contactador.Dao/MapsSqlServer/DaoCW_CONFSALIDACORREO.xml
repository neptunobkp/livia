		SERVIDORSMTP,
		PUERTOSERVIDOR,
		SSLHABILITADO,
		USUARIO,
		CONTRASENIA,
		CORREOCASILLASALIDA,
		NOMBRECASILLASALDA,
		IDPROPIETARIO,
		TIPOPROPIETARIO,
		USUARIOCREADOR,
		FECHACREACION,
		USUARIOMODIFICADOR,
		FECHAMODIFICACION)
	VALUES(
		#SERVIDORSMTP#,
		#PUERTOSERVIDOR#,
		#SSLHABILITADO#,
		#USUARIO#,
		#CONTRASENIA#,
		#CORREOCASILLASALIDA#,
		#NOMBRECASILLASALDA#,
		#IDPROPIETARIO#,
		#TIPOPROPIETARIO#,
